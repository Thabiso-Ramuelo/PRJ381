<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Student_Analyser.Views.Home.License_view"
             BackgroundColor="Black" Title="License"
             NavigationPage.HasBackButton="False">
    <ContentPage.Content>
        <ScrollView Padding="5,2,5,2">
            <Label x:Name="licenseLabel" Text="{Binding .}"
                   TextColor="#275FDA"
                   BackgroundColor="Black"/>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>